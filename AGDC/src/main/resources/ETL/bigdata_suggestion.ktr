    <connection>zhxn_v2</connection>
    <sql>select case when a.region_type=3 then a.region_name else null end as district_name,

case when b.region_type=2 then CONCAT(b.region_name,'市') when b.region_type=1 then CONCAT(a.region_name,'市') else null end as city_name,

case when c.region_type=1 then 
case when c.region_name in ('北京','天津','重庆','上海') then CONCAT(c.region_name,'市') when c.region_name ='新疆' then CONCAT(c.region_name,'维吾尔自治区') when c.region_name ='宁夏' then CONCAT(c.region_name,'回族自治区') when c.region_name in ('西藏','内蒙古') then CONCAT(c.region_name,'自治区') when c.region_name ='广西' then CONCAT(c.region_name,'壮族自治区') else CONCAT(c.region_name,'省') end 
when c.region_type=0 then 
case when b.region_name in ('北京','天津','重庆','上海') then CONCAT(b.region_name,'市') when b.region_name ='新疆' then CONCAT(b.region_name,'维吾尔自治区') when b.region_name ='宁夏' then CONCAT(b.region_name,'回族自治区') when b.region_name in ('西藏','内蒙古') then CONCAT(b.region_name,'自治区') when b.region_name ='广西' then CONCAT(b.region_name,'壮族自治区')else CONCAT(b.region_name,'省') end 
else case when a.region_name in ('北京','天津','重庆','上海') then CONCAT(a.region_name,'市') when a.region_name ='新疆' then CONCAT(a.region_name,'维吾尔自治区') when a.region_name ='宁夏' then CONCAT(a.region_name,'回族自治区') when a.region_name in ('西藏','内蒙古') then CONCAT(a.region_name,'自治区') when b.region_name ='广西' then CONCAT(a.region_name,'壮族自治区')else CONCAT(a.region_name,'省') end end as province_name,
s.id
from zhxn_product p LEFT JOIN zhxn_region a on a.region_id =p.region_id
LEFT JOIN zhxn_region b on a.parent_id = b.region_id
LEFT JOIN zhxn_region c on b.parent_id = c.region_id
INNER JOIN zhxn_suggestion s on s.product_id=p.id 
where p.is_suggestion=1;</sql>
